<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
        <key>Label</key>
        <string>com.simplebackup.foo</string>
        <key>ProgramArguments</key>
        <array>
                <string>/Users/foo/backup.sh</string>
        </array>
        <key>StartCalendarInterval</key>
        <dict>
                <key>Hour</key>
                <integer>9</integer>
                <key>Minute</key>
                <integer>00</integer>
        </dict>
        <key>StandardOutPath</key>
        <string>/Users/foo/Library/Logs/com.simplebackup.foo_out.log</string>
        <key>StandardErrorPath</key>
        <string>/Users/foo/Library/Logs/com.simplebackup.foo_err.log</string>
</dict>
</plist>